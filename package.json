{
  "name": "nodejs-typescript-setup",
  "version": "1.0.0",
  "description": "Setup to NodeJs projects",
  "main": "index.js",
  "scripts": {
    "dev": "tsx --watch --no-warnings src/index.ts",
    "start": "node build/index.js",
    "build": "babel src --extensions \".ts,.js\" --out-dir build --copy-files",
    "fm": "prettier --write src",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luchooo/nodejs-typescript-setup.git"
  },
  "author": "Luis Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Luchooo/nodejs-typescript-setup/issues"
  },
  "homepage": "https://github.com/Luchooo/nodejs-typescript-setup#readme",
  "engines": {
    "node": "20.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/morgan": "1.9.9",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "prettier": "3.2.4",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "express": "4.18.2",
    "morgan": "1.10.0",
    "winston": "3.11.0"
  }
}
